-- 业务信息
CREATE TABLE Business (
    id INTEGER PRIMARY KEY NOT NULL ,
    businessName TEXT UNIQUE NOT NULL,
    created INTEGER NOT NULL DEFAULT (strftime('%s', 'now'))
);

selectAll:
SELECT * FROM Business;

selectByName:
SELECT * FROM Business
WHERE businessName = ?;

insert:
INSERT INTO Business(id,businessName)
VALUES (?,?);

update:
UPDATE Business
SET businessName = ?
WHERE id = ?;



