CREATE TABLE RelBizTpl (
    id INTEGER NOT NULL ,
    businessId INTEGER NOT NULL REFERENCES Business(id),
    templateId INTEGER NOT NULL REFERENCES Template(id),
    created INTEGER NOT NULL DEFAULT (strftime('%s', 'now')),
    PRIMARY KEY (businessId,templateId)
);

selectByBusinessAndTemplate:
SELECT *
FROM RelBizTpl
WHERE businessId=? AND templateId=?;

insert:
INSERT INTO RelBizTpl(id,businessId,templateId)
VALUES (?,?,?);